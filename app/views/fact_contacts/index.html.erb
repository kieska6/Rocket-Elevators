<p id="notice"><%= notice %></p>

<body class="text-center smoothscroll enable-animation">

<h1 class="display-5 fw-bold text-center">Fact Contacts</h1>

<table class= "container table table-primary table-striped">
  <thead>
    <tr>
      <th>Contact ID</th>
      <th>Creation Date</th>
      <th>Company Name</th>
      <th>Email</th>
      <th>Project Name</th>
      <th>Time Dimension ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fact_contacts.each do |fact_contact| %>
      <tr>
        <td><%= fact_contact.ContactId %></td>
        <td><%= fact_contact.Creation_Date %></td>
        <td><%= fact_contact.Company_Name %></td>
        <td><%= fact_contact.Email %></td>
        <td><%= fact_contact.Project_Name %></td>
        <td><%= fact_contact.dim_time_id %></td>
        <td><%= link_to 'Show', fact_contact %></td>
        <td><%= link_to 'Edit', edit_fact_contact_path(fact_contact) %></td>
        <td><%= link_to 'Destroy', fact_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fact Contact', new_fact_contact_path %>
