<p id="notice"><%= notice %></p>

<h1>Fact Interventions</h1>

<table>
  <thead>
    <tr>
      <th>Employee</th>
      <th>Building</th>
      <th>Battery</th>
      <th>Column</th>
      <th>Elevator</th>
      <th>Intervention start date time</th>
      <th>Intervention end date time</th>
      <th>Result</th>
      <th>Report</th>
      <th>Status opt</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fact_interventions.each do |fact_intervention| %>
      <tr>
        <td><%= fact_intervention.employee_id %></td>
        <td><%= fact_intervention.building_id %></td>
        <td><%= fact_intervention.battery_id %></td>
        <td><%= fact_intervention.column_id %></td>
        <td><%= fact_intervention.elevator_id %></td>
        <td><%= fact_intervention.intervention_start_date_time %></td>
        <td><%= fact_intervention.intervention_end_date_time %></td>
        <td><%= fact_intervention.result %></td>
        <td><%= fact_intervention.report %></td>
        <td><%= fact_intervention.status_opt %></td>
        <td><%= link_to 'Show', fact_intervention %></td>
        <td><%= link_to 'Edit', edit_fact_intervention_path(fact_intervention) %></td>
        <td><%= link_to 'Destroy', fact_intervention, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fact Intervention', new_fact_intervention_path %>
